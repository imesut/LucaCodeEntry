{
    "manifest_version": 2,
    "name": "AccountingCodes v1.2",
    "version": "1.2",
    "browser_action": {
        "default_title": "Accounting Codes"
    },
    "description": "A Chrome extension, makes code entry easy and secure for Luca",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [{
        "matches": [
            "*://*.luca.com.tr/*"
        ],
        "js": ["code-list.js","jquery.min.js", "popup.js"],
        "run_at": "document_end",
        "all_frames": true
    }]
}