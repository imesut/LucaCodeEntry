{
    "manifest_version": 2,
    "name": "Luca Kod Giriş Yardımcısı v2",
    "version": "2.0",
    "browser_action": {
        "default_title": "Luca Kod Giriş Yardımcısı"
    },
    "content_security_policy": "script-src 'self' 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs='",
    "description": "A Chrome extension, makes code entry easy and secure for Luca",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
    ],
    "options_page": "options.html",
    "content_scripts": [{
        "matches": [
            "*://*.luca.com.tr/*"
        ],
        "js": ["code-list.js","jquery.min.js", "popup.js", "options.js"],
        "run_at": "document_end",
        "all_frames": true
    }]
}